@model IEnumerable<Report_Wlk>;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Report</title>
    <link href="https://cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/buttons/2.2.3/css/buttons.dataTables.min.css" rel="stylesheet" />
    
    <script src="https://cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script> 
    <script src="https://cdn.datatables.net/buttons/2.3.3/js/dataTables.buttons.min.js"></script> 
    <script src="https://cdnjs.cloudfare.com/ajax/libs/jszip/3.1.3/jzip.min.js"></script>

    <script src="https://cdnjs.cloudfare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>

    <script src="https://cdnjs.cloudfare.com/ajax/libs/pdfmake/0.1.53/vfsfonts.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.3/js/buttons.html5.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.3/js/buttons.print.min.js"></script>



    <style>
      
    </style>
</head>
<body>
    
    <div class="text-center">
        <h1 class="display-4">Report to CSV, PDF, PRINT</h1>
        <div class = "cntnt">
            <table id="exmpl" class="display nowrap" style="width:100%">
                <thead>
                    <td>Name</td
                    <td>Surname</td>
                </thead>/>

                <tbody>
                    @foreach(var x in Model)
                    {
                        <tr>
                            <td> @x.Name</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>

    <script>
$(document).ready(function() {
    $('#exmpl').DataTable({
        dom: 'lBfrtip',
        buttons: ['copy', 'csv', 'excel', 'pdf', 'print']
    });
});
    </script>

</body>
</html>



