@model Checkup;
<link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />

@{
    ViewData["Title"] = "View Appointments";
}
<body id="home">
    <h1 id="bookh1">View CheckUps</h1>
    <p>
        <a asp-action="Index">Back to Checkup List</a>
    </p>

    <div>
        <dl class="row">
            <dt class="col-sm-2">
                Checkup Date
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CheckupDate)
            </dd>
            <dt class="col-sm-2">
                Results
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Results)
            </dd>
            <dt class="col-sm-2">
                Notes
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Notes)
            </dd>
        </dl>
    </div>
   
    <br />
    <a asp-controller="Booking" asp-action="ViewBookings"><button id="button1" class="btn-danger">Back</button></a>

</body>


