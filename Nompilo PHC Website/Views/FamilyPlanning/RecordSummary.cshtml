                                            OvuLating = field.Ovulation1;
                                            FirstRisk = field.PregnancyPrediction;
                                            LastRisk = field.PregLastDay;
                                            
                                        }
                                        <tr style="width:fit-content;background-color:lightblue">
                                            <th>
                                               Period Start Date
                                            </th>
                                            <th>
                                               Period End Date
                                            </th>
                                            <th>
                                               Period Normality
                                            </th>
                                            <td>
                                            </td>
                                            <td>
                                            </td>
                                            <td>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>@StartDate.ToString("dd MMMM yyyy")</td>
                                            <td>@EndDate.ToString("dd MMMM yyyy")</td>
                                            <td>@Status</td>
                                            <td>
                                                <a type="submit" asp-controller="FamilyPlanning" asp-action="LogUpdate" asp-route-ID="@ID" class=" btn btn-primary" id="AllbutB">Edit End Date</a>
                                            </td>
                                            <td>
                                                <a asp-controller="FamilyPlanning" asp-action="LogDelete" asp-route-ID="@ID" class="btn btn-primary" id="AllbutB">Delete</a>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="6" style="width:fit-content;background-color:white">
                                                <hr>
                                                <p>Note: The information below is based on your logged period and only applies for the month of @MonthName of the current year.The nurse will use the infomation below to prescribe a suitable Morning After pill for you ensure you log your periods every month incase you will need to use morning after pill. You should avoid having unprotected sex on the date mentioned below unless you are on a contraceptive as your fertility is at its peak, failer to do so visit a nurse for morning after pills within 72 hours.<b>These are Estimations</b></p>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td style="width:fit-content;background-color:lightblue" colspan="6">
                                                <b>This Month's Fertility Window (@MonthName)</b>
                                                <br />
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="2">
                                                <b>
                                                    Ovulation  is on the:
                                                </b>
                                            </td>
                                            <td>@OvuLating.ToString("dd MMMM yyyy")</td>
                                            <td colspan="3"></td>
                                            
                                        </tr>
                                        <tr>
                                            
                                    
                                        </tr>

                                      
                                    </tbody>
                                </table>
                            </div>
                        }
                        else
                        {
                            <table class="table-borderless">
                                <tbody>
                                    <tr>

                                        <label>No record found! Click the button below to log period</label>
                                        <br />
                                    </tr>

                                    <tr>

                                        <div class="col-12" align="center">
                                            <button asp-controller="FamilyPlanning" asp-action="LogPeriod" id="AllbutF">Log Period</button>

                                        </div>
                                    </tr>
                                </tbody>
                            </table>
                        }
                    }
                    else
                    {
                        <table class="table-borderless">
                            <tbody>
                                <tr>

                                    <label>No record found! Click the button below to log period first</label>
                                    <br />
                                </tr>

                                <tr>

                                    <div class="col-12" align="center">

                                        <button asp-controller="FamilyPlanning" asp-action="LogPeriod" id="AllbutF">Log Period</button>

                                    </div>
                                </tr>
                            </tbody>
                        </table>
                    }


                </form>
            </div>
        </section>


    </section>
